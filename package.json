{
  	"name": "spwnin",
  	"displayName": "spwnIn",
  	"description": "LSP for spwn gd",
  	"version": "1.1.0",
  	"publisher": "Avaneesh",
  	"repository": {
  	  	"url": "https://github.com/Avaneesh-coder/SpwnIn"
  	},
  	"engines": {
  	  	"vscode": "^1.59.0"
  	},
	  "icon": "./images/spwn_logo.png",
  	"categories": [
  	  	"Other"
  	],
  	"activationEvents": [
  	  	"*"
  	],
  	"main": "./out/extension.js",
  	"contributes": {
  	  	"commands": [
  	  	  	{
  	  	  	  	"command": "spwnin.bobcode",
  	  	  	  	"title": "Sample BOB Code"
  	  	  	},
  	  	  	{
  	  	  	  	"command": "spwnin.ontouch",
  	  	  	  	"title": "Sample on touch function(OLD/outdated)"
  	  	  	},
			{
				"command": "spwnin.doughnut",
				"title": "donut code"
			}
  	  	],
  	  	"languages": [
  	  	  	{
  	  	  	  	"id": "spwn",
  	  	  	  	"aliases": [
  	  	  	  	  	"Spwn",
  	  	  	  	  	"spwn"
  	  	  	  	],
  	  	  	  	"extensions": [
  	  	  	  	  	".spwn"
  	  	  	  	],
  	  	  	  	"configuration": "./language-configuration.json"
  	  	  	}
  	  	],
  	  	"grammars": [
  	  	  	{
  	  	  	  	"language": "spwn",
  	  	  	  	"scopeName": "source.spwn",
  	  	  	  	"path": "./syntaxes/spwn.tmLanguage.json"
  	  	  	}
  	  	],
  	  	"snippets": [
  	  	  	{
  	  	  	  	"language": "spwn",
  	  	  	  	"path": "./snippets/snippets.code-snippets"
  	  	  	}
  	  	],
		"iconThemes": [
			{
				"id": "spwn-icon",
				"label": "icon for spwn",
				"path": "spwn-icon.json"
			}
		]
  	},
  	"scripts": {
  	  	"vscode:prepublish": "npm run compile",
  	  	"compile": "tsc -p ./",
  	  	"watch": "tsc -watch -p ./",
  	  	"pretest": "npm run compile && npm run lint",
  	  	"lint": "eslint src --ext ts",
  	  	"test": "node ./out/test/runTest.js"
  	},
  	"devDependencies": {
  	  	"@types/glob": "^7.1.3",
  	  	"@types/mocha": "^8.2.2",
  	  	"@types/node": "14.x",
  	  	"@types/vscode": "^1.59.0",
  	  	"@typescript-eslint/eslint-plugin": "^4.26.0",
  	  	"@typescript-eslint/parser": "^4.26.0",
  	  	"eslint": "^7.27.0",
  	  	"glob": "^7.1.7",
  	  	"mocha": "^8.4.0",
  	  	"typescript": "^4.3.2",
  	  	"vscode-test": "^1.5.2"
  	},
  	"dependencies": {
  	  	"vscode-languageclient": "^7.0.0"
  	}
}
