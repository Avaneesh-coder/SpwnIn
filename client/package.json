{
	"name": "spwnin",
  	"displayName": "spwnIn",
  	"description": "LSP for spwn gd",
  	"version": "1.1.0",
  	"publisher": "Avaneesh",
	"repository": {
		"type": "git",
		"url": "https://github.com/Avaneesh-coder/SpwnIn"
	},
	"engines": {
		"vscode": "^1.59.0"
	},
	"icon": "images/spwn_logo.png",
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "spwnin.bobcode",
				"category": "Spwn",
				"title": "Sample BOB Code"
			},
			{
				"command": "spwnin.ontouch",
				"category": "Spwn",
				"title": "Sample on touch function(OLD/outdated)"
			},
			{
				"command": "spwnin.doughnut",
				"category": "Spwn",
				"title": "donut code"
			}
			],
			"languages": [
				{
					"id": "spwn",
					"aliases": [
						"Spwn",
						"spwn"
					],
					"extensions": [
						".spwn"
					],
					"configuration": "./language-configuration.json"
				}
			],
			"grammars": [
				{
					"language": "spwn",
					"scopeName": "source.spwn",
					"path": "./syntaxes/spwn.tmLanguage.json"
				}
			],
			"snippets": [
				{
					"language": "spwn",
					"path": "./snippets/snippets.code-snippets"
				}
			],
		"iconThemes": [
			{
				"id": "spwn-icon",
				"label": "icon for spwn",
				"path": "spwn-icon.json"
			}
		]
	},

	"dependencies": {
		"vscode-languageclient": "^7.0.0"
	},
	"devDependencies": {
		"@types/vscode": "^1.52.0",
		"@vscode/test-electron": "^1.6.1"
	}
}
