{
	"name": "spwnin",
  	"displayName": "spwnIn",
  	"description": "LSP for spwn gd",
  	"version": "1.1.0",
  	"publisher": "Avaneesh",
	"repository": {
		"type": "git",
		"url": "https://github.com/Avaneesh-coder/SpwnIn"
	},
	"engines": {
		"vscode": "^1.59.0"
	},
	"icon": "images/spwn_logo.png",
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:spwn"
	],
	"main": "./out/extension.js",
	"contributes": {
		"configuration": {
			"type": "object",
			"title": "Multi LSP configuration",
			"properties": {
				"lspMultiServerSample.enable": {
					"scope": "resource",
					"type": "boolean",
					"default": true,
					"description": "Controls the enablement."
				},
				"lspMultiServerSample.options": {
					"scope": "resource",
					"type": "object",
					"default": {},
					"description": "Additional options."
				},
				"lspMultiServerSample.trace.server": {
					"scope": "window",
					"type": "string",
					"enum": [
						"off",
						"messages",
						"verbose"
					],
					"default": "off",
					"description": "Traces the communication between VSCode and the language server."
				}
			}
		}
	},
	"languages": [
		{
			"id": "spwn",
			"aliases": [
				"Spwn",
				"spwn"
			],
			"extensions": [
				".spwn"
			],
			"configuration": "./language-configuration.json"
		}
	],
	"grammars": [
		{
			"language": "spwn",
			"scopeName": "source.spwn",
			"path": "./syntaxes/spwn.tmLanguage.json"
		}
	],
	"scripts": {},
	"dependencies": {
		"vscode-languageclient": "^7.0.0"
	},
	"devDependencies": {
		"@types/vscode": "^1.52.0"
	}
}
